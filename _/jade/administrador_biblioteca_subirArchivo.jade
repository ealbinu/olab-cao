include _mixins.jade
doctype html
html(lang="es")
	head
		+head("CAO | Administrador")
body
	#all.toggled-right
		header.mainHeader
			+header('Administrador')
				.container-fluid.bg-info.subtitle.text-info
					.col-xs-12
						.lead Subir archivos en la biblioteca general
					
		+sidebarLeft('administrador')
		+sidebarRight
		
		
		
			
				
				
		#page-content-wrapper
			.content-fluid
				.row
					.col-lg-8.col-lg-push-2
						
						.panel.panel-primary
							.panel-heading: i.fa.fa-upload  Arrastre un archivo para cargarlo a la biblioteca
							.panel-body
								form.dropzone#administrador_subirArchivo(action="#")
									.dz-message Arrastre o de click
							.panel-footer: i.fa.fa-file  Archivos soportados: .pdf, .xls, .xlsx, .png, .jpg. jpeg
		

		+sharedScripts
